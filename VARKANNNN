function Main()
  menu1 = gg.choice({
    "╔✧[𝙱𝚈𝙿𝙰𝚂𝚂]     \n╚✧[𝙻𝙾𝙱𝙱𝚈✧📤]",	
    "╔✧[ᎷᎬΝႮ]      \n╚✧[ᏀᎪᎷᎬ✧📤]",
    "╔✧\n╚✧🅴🆇🅸🆃"
  }, nil, (os.date(" 𝚅𝙴𝚁𝚂𝙸𝙾𝙽 =15.0    𝙶𝙰𝙼𝙰𝚃𝙰𝚁𝚄  \n『🆅』@GAMATARU\n『🅸』𝙲𝙷𝙸𝙽𝙰𝙿𝚄𝙱𝙶𝙼𝙾𝙱𝙸𝙻𝙴𝙷𝙰𝙲𝙺𝚂 \n『🅿』ͲᎻᎪΝᏦՏ ҒϴᎡ ᎽϴႮᎡ ՏႮᏢᏢϴᎡͲ")))
if menu1 == nil then
  else
  if menu1 == 1 then
    BYPASS()
  end
  if menu1 == 2 then
    D()
  end
  if menu1 == 3 then
    Exit()
    end
  end
  XGCK = -1
end
function BYPASS()
RX = gg.prompt({
'🛡️𝙿𝚄𝙱𝙻𝙸𝙲 𝙱𝚁𝚄𝚃𝙰𝙻[GLOBAL] 𝙻𝙾𝙱𝙱𝚈',
'🛡️𝙿𝚄𝙱𝙻𝙸𝙲 𝙱𝚁𝚄𝚃𝙰𝙻[KOREA] 𝙻𝙾𝙱𝙱𝚈',
'🔚EXIT🔚[GAMATARU]',}, {}, {"checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox"})
if RX == nil then else
if RX[1] == true then GLOBAL()end
if RX[2] == true then KOREA()end
if RX[3] == true then Main()end
end
RXY=0
end


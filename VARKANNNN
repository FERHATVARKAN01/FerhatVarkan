function Main()
  menu1 = gg.choice({
    "‚ïî‚úß[ùô±ùöàùôøùô∞ùöÇùöÇ]     \n‚ïö‚úß[ùôªùôæùô±ùô±ùöà‚úßüì§]",	
    "‚ïî‚úß[·é∑·é¨Œù·ÇÆ]      \n‚ïö‚úß[·èÄ·é™·é∑·é¨‚úßüì§]",
    "‚ïî‚úß\n‚ïö‚úßüÖ¥üÜáüÖ∏üÜÉ"
  }, nil, (os.date(" ùöÖùô¥ùöÅùöÇùô∏ùôæùôΩ =15.0    ùô∂ùô∞ùôºùô∞ùöÉùô∞ùöÅùöÑ  \n„ÄéüÜÖ„Äè@GAMATARU\n„ÄéüÖ∏„Äèùô≤ùô∑ùô∏ùôΩùô∞ùôøùöÑùô±ùô∂ùôºùôæùô±ùô∏ùôªùô¥ùô∑ùô∞ùô≤ùô∫ùöÇ \n„ÄéüÖø„ÄèÕ≤·éª·é™Œù·è¶’è “íœ¥·é° ·éΩœ¥·ÇÆ·é° ’è·ÇÆ·è¢·è¢œ¥·é°Õ≤")))
if menu1 == nil then
  else
  if menu1 == 1 then
    BYPASS()
  end
  if menu1 == 2 then
    D()
  end
  if menu1 == 3 then
    Exit()
    end
  end
  XGCK = -1
end
function BYPASS()
RX = gg.prompt({
'üõ°Ô∏èùôøùöÑùô±ùôªùô∏ùô≤ ùô±ùöÅùöÑùöÉùô∞ùôª[GLOBAL] ùôªùôæùô±ùô±ùöà',
'üõ°Ô∏èùôøùöÑùô±ùôªùô∏ùô≤ ùô±ùöÅùöÑùöÉùô∞ùôª[KOREA] ùôªùôæùô±ùô±ùöà',
'üîöEXITüîö[GAMATARU]',}, {}, {"checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox","checkbox"})
if RX == nil then else
if RX[1] == true then GLOBAL()end
if RX[2] == true then KOREA()end
if RX[3] == true then Main()end
end
RXY=0
end


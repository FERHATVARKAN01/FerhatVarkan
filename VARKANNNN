function D()
  GAMAT = gg.multiChoice({
    "╔✧[ʟᴏʙʙʏ] 『👤』\n╚✧ʜ̴̴ᴇ̴̴ᴀ̴̴ᴅ̴̴s̴̴ʜ̴̴ᴏ̴̴ᴛ̴",
    "╔✧[ʟᴏʙʙʏ] 『⚡』\n╚✧ᴄ̴̴ʀ̴̴ᴏ̴̴s̴̴s̴ ̴ʜ̴̴ᴀ̴̴ɪ̴̴ʀ̴",
    "╔✧[ʟᴏʙʙʏ] 『🍹』\n╚✧ʟ̴̴ᴇ̴̴s̴̴s̴ ̴ʀ̴̴ᴇ̴̴ᴄ̴̴ᴏ̴̴ɪ̴̴ʟ̴",
    "╔✧[ʟᴏʙʙʏ] 『🌪️』\n╚✧ᴀ̴̴ɴ̴̴ᴛ̴̴ɪ̴ ̴s̴̴ʜ̴̴ᴀ̴̴ᴋ̴̴ᴇ̴",
    "╔✧[GAME] 『🌑』\n╚✧𝙵𝙰𝚂𝚃 𝙿𝙰𝚁𝙰𝙲𝙷𝚄𝚃𝙴",
    "╔✧[ʟᴏʙʙʏ] 『💫』\n╚✧W̴̴i̴̴d̴̴e̴ ̴v̴̴i̴̴e̴̴w̴",
    "╔✧\n╚☩🅱🅰🅲🅺"
  }, nil, "━━━━━━━━━━━━━━━━━━━━━\n┏  @GAMATARU\n━━━━━━━━━━━━━━━━━━━━━")
  if GAMAT == nil then
  else
    if GAMAT[1] == true then
      HEAD()
    end
    if GAMAT[2] == true then
      CROSS()
    end
    if GAMAT[3] == true then
      LESS()
    end
    if GAMAT[4] == true then
      SHAKE()
    end
    if GAMAT[5] == true then
      BLACK()
    end
    if GAMAT[6] == true then
      WIDE()
    end
    if GAMAT[7] == true then
      Main()
    end
  end
  XGCK = -1
end

